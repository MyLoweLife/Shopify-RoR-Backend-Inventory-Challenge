
<p id="notice"><%= notice %></p>

<table class="table  table-striped" >

  <thead>
    <tr>
      <th>Product</th>
      <th>Color</th>
      <th>Size</th>
      <th>Quantity</th>
      <th>Location</th>
      <th>Created At</th>
      <th>Updated At</th>
      <th>Marked For Deletion?</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @logistics.each do |logistic| %>
      <tr>
        <td><%= logistic.product %></td>
        <td><%= logistic.color %></td>
        <td><%= logistic.size %></td>
        <td><%= logistic.quantity %></td>
        <td><%= logistic.location %></td>
        <td><%= logistic.created_at %></td>
        <td><%= logistic.updated_at %></td>
        <th><%= logistic.discarded? ? 'YES':'NO' %></th>
        <td><%= link_to 'Show', logistic %></td>
        <td><%= link_to 'Edit', edit_logistic_path(logistic) %></td>
        <td><%= link_to 'Destroy', logistic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Add Inventory', new_logistic_path, class:"btn btn-outline-secondary" %>
